{
  "name": "vue-project",
  "version": "0.0.0",
  "scripts": {
    "prepare": "cd .. && husky install",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "test": "jest",
    "lint:script": "eslint --fix src/**/*.{js,ts,json,jsx,tsx,vue} --no-error-on-unmatched-pattern",
    "lint:styles": "npx stylelint \"src/**/*.scss\" --fix",
    "format:all": "prettier --write '**/*.{md,ts,html,css,scss,json,vue}'",
    "format": "prettier --write"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@popperjs/core": "^2.11.8",
    "@types/vue": "^2.0.0",
    "@vue/babel-preset-app": "^5.0.8",
    "@vue/compiler-dom": "^3.2.47",
    "@vuepic/vue-datepicker": "^4.2.3",
    "axios": "^1.3.4",
    "bootstrap": "^5.3.1",
    "decode-uri-component": "^0.2.1",
    "echarts": "^5.4.2",
    "git-commit-msg-linter": "^5.0.4",
    "glob-parent": "^5.1.2",
    "gulp": "^4.0.2",
    "gulp-less": "^5.0.0",
    "gulp-sass": "^5.1.0",
    "http-cache-semantics": "^4.1.1",
    "husky": "^8.0.3",
    "json5": "^2.2.2",
    "lint-staged": "^14.0.1",
    "moment": "^2.29.4",
    "pinia": "^2.0.32",
    "sharp": "^0.32.6",
    "v-calendar": "^3.0.0-alpha.8",
    "validate-branch-name": "^1.3.0",
    "vue": "^3.2.47",
    "vue-echarts": "^6.5.4",
    "vue-router": "^4.1.6",
    "vue-select": "^4.0.0-beta.6"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.18.13",
    "@types/vue-select": "^3.16.2",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "jest": "^27.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "sass": "^1.58.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.3.1",
    "ts-jest": "^27.0.4",
    "typescript": "~4.7.4",
    "vite": "^3.2.5",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.2.0"
  },
  "tasks": {
    "label": "Sass Compile",
    "type": "shell",
    "command": "sass styles.scss styles.css",
    "group": "build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"> husky pre-commit\"",
      "prepare-commit-msg": "echo \"> husky pre-commit\""
    }
  }
}
